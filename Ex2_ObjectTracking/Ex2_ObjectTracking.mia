&#10;&#10;Here, this timeseries is simply named &quot;Timeseries&quot;.&#10;&#10;All parameters are left as their default values, which will load all frames.&#10;&#10;Something to note is that the timeseries isn't spatially or temporally calibrated, so calibrations of &quot;1&quot; will be applied.  As such, any &quot;spatially calibrated&quot; measurements will actually be in pixel units." SHOW_BASIC_TITLE="true" SHOW_OUTPUT="false" VERSION="1.0.0"><PARAMETERS><PARAMETER NAME="Output image" NICKNAME="Output image" VALUE="Timeseries" VISIBLE="false"/><PARAMETER NAME="Import mode" NICKNAME="Import mode" VALUE="Current file" VISIBLE="false"/><PARAMETER NAME="Reader" NICKNAME="Reader" VALUE="Bio-Formats" VISIBLE="false"/><PARAMETER NAME="Sequence root name" NICKNAME="Sequence root name" VALUE="" VISIBLE="false"/><PARAMETER NAME="Name format" NICKNAME="Name format" VALUE="Generic (from metadata)" VISIBLE="false"/><PARAMETER NAME="Comment" NICKNAME="Comment" VALUE="" VISIBLE="false"/><PARAMETER NAME="Extension" NICKNAME="Extension" VALUE="" VISIBLE="false"/><PARAMETER NAME="Generic format" NICKNAME="Generic format" VALUE="" VISIBLE="false"/><PARAMETER NAME="Include series number" NICKNAME="Include series number" VALUE="true" VISIBLE="false"/><PARAMETER NAME="File path" NICKNAME="File path" VALUE="" VISIBLE="false"/><PARAMETER NAME="Series mode" NICKNAME="Series mode" VALUE="Current series" VISIBLE="false"/><PARAMETER NAME="Series number" NICKNAME="Series number" VALUE="1" VISIBLE="false"/><PARAMETER NAME="Channels" NICKNAME="Channels" VALUE="1-end" VISIBLE="false"/><PARAMETER NAME="Slices" NICKNAME="Slices" VALUE="1-end" VISIBLE="false"/><PARAMETER NAME="Frames" NICKNAME="Frames" VALUE="1-end" VISIBLE="false"/><PARAMETER NAME="Channel" NICKNAME="Channel" VALUE="1" VISIBLE="false"/><PARAMETER NAME="Crop mode" NICKNAME="Crop mode" VALUE="None" VISIBLE="false"/><PARAMETER NAME="Reference image" NICKNAME="Reference image" VALUE="" VISIBLE="false"/><PARAMETER NAME="Left coordinate" NICKNAME="Left coordinate" VALUE="0" VISIBLE="false"/><PARAMETER NAME="Top coordinate" NICKNAME="Top coordinate" VALUE="0" VISIBLE="false"/><PARAMETER NAME="Width" NICKNAME="Width" VALUE="512" VISIBLE="false"/><PARAMETER NAME="Height" NICKNAME="Height" VALUE="512" VISIBLE="false"/><PARAMETER NAME="Objects for limits" NICKNAME="Objects for limits" VALUE="" VISIBLE="false"/><PARAMETER NAME="Scale mode" NICKNAME="Scale mode" VALUE="No scaling" VISIBLE="false"/><PARAMETER NAME="X scale factor" NICKNAME="X scale factor" VALUE="4" VISIBLE="false"/><PARAMETER NAME="Y scale factor" NICKNAME="Y scale factor" VALUE="4" VISIBLE="false"/><PARAMETER NAME="Dimension mismatch mode" NICKNAME="Dimension mismatch mode" VALUE="Disallow (fail upon mismatch)" VISIBLE="false"/><PARAMETER NAME="Pad intensity mode" NICKNAME="Pad intensity mode" VALUE="Black (0)" VISIBLE="false"/><PARAMETER NAME="Set manual spatial calibration" NICKNAME="Set manual spatial calibration" VALUE="false" VISIBLE="false"/><PARAMETER NAME="XY calibration (dist/px)" NICKNAME="XY calibration (dist/px)" VALUE="1.0" VISIBLE="false"/><PARAMETER NAME="Z calibration (dist/px)" NICKNAME="Z calibration (dist/px)" VALUE="1.0" VISIBLE="false"/><PARAMETER NAME="Set manual temporal calibration" NICKNAME="Set manual temporal calibration" VALUE="false" VISIBLE="false"/><PARAMETER NAME="Frame interval (time/frame)" NICKNAME="Frame interval (time/frame)" VALUE="1.0" VISIBLE="false"/><PARAMETER NAME="Force bit depth" NICKNAME="Force bit depth" VALUE="false" VISIBLE="false"/><PARAMETER NAME="Output bit depth" NICKNAME="Output bit depth" VALUE="8" VISIBLE="false"/><PARAMETER NAME="Minimum input intensity" NICKNAME="Minimum input intensity" VALUE="0.0" VISIBLE="false"/><PARAMETER NAME="Maximum input intensity" NICKNAME="Maximum input intensity" VALUE="1.0" VISIBLE="false"/></PARAMETERS><IMAGE_MEASUREMENTS/><OBJECT_MEASUREMENTS/><METADATA/></MODULE><MODULE CLASSNAME="io.github.mianalysis.mia.module.system.GUISeparator" DISABLEABLE="false" ENABLED="true" ID="1694380732098" NAME="GUI separator" NICKNAME="Detection and tracking" NOTES="" SHOW_BASIC_TITLE="true" SHOW_OUTPUT="false" VERSION="1.0.0"><PARAMETERS><PARAMETER NAME="Show in processing view" NICKNAME="Show basic" VALUE="true" VISIBLE="false"/><PARAMETER NAME="Expanded in processing view" NICKNAME="Expanded basic GUI" VALUE="true" VISIBLE="false"/><PARAMETER NAME="Expanded in editing view" NICKNAME="Expanded editing GUI" VALUE="true" VISIBLE="false"/></PARAMETERS><IMAGE_MEASUREMENTS/><OBJECT_MEASUREMENTS/><METADATA/></MODULE><MODULE CLASSNAME="io.github.mianalysis.mia.module.objects.detect.SpotDetection" DISABLEABLE="false" ENABLED="true" ID="1694378043371" NAME="Spot detection" NICKNAME="Spot detection" NOTES="The &quot;Spot detection&quot; module uses TrackMate's Laplacian of Gaussian (LoG) detector.&#10;&#10;With this module, each frame of the input timeseries is filtered using a LoG filter with the defined radius (specific in pixel units).  This enhances spot-like features of that particular size.&#10;&#10;From the filtered image, any peaks above the specified threshold will be identified as &quot;Nuclei&quot; objects, which are then added to MIA's workspace for use in downstream modules.  Objects output by the &quot;Spot detection&quot; module contain a single coordinate closest to the detected centre of the spot.&#10;&#10;Although not used here, if dealing with 3D timeseries, &quot;spots&quot; can either be detected on a per-slice basis or in 3D using the &quot;Detection mode&quot; parameter.&#10;&#10;" SHOW_BASIC_TITLE="true" SHOW_OUTPUT="false" VERSION="1.0.0"><PARAMETERS><PARAMETER NAME="Input image" NICKNAME="Input image" VALUE="Timeseries" VISIBLE="false"/><PARAMETER NAME="Output spot objects" NICKNAME="Output spot objects" VALUE="Nuclei" VISIBLE="false"/><PARAMETER NAME="Detection mode" NICKNAME="Detection mode" VALUE="3D" VISIBLE="false"/><PARAMETER NAME="Calibrated units" NICKNAME="Calibrated units" VALUE="false" VISIBLE="false"/><PARAMETER NAME="Do sub-pixel localisation" NICKNAME="Do sub-pixel localisation" VALUE="true" VISIBLE="false"/><PARAMETER NAME="Median filtering" NICKNAME="Median filtering" VALUE="false" VISIBLE="false"/><PARAMETER NAME="Radius" NICKNAME="Radius" VALUE="3" VISIBLE="false"/><PARAMETER NAME="Threshold" NICKNAME="Threshold" VALUE="10" VISIBLE="false"/><PARAMETER NAME="Estimate spot size" NICKNAME="Estimate spot size" VALUE="false" VISIBLE="false"/></PARAMETERS><IMAGE_MEASUREMENTS/><OBJECT_MEASUREMENTS><MEASUREMENT EXPORT_GLOBAL="false" EXPORT_INDIVIDUAL="true" EXPORT_MAX="true" EXPORT_MEAN="true" EXPORT_MIN="true" EXPORT_STD="true" EXPORT_SUM="true" NAME="SPOT_DETECT // QUALITY" NICKNAME="SPOT_DETECT // QUALITY" OBJECT_NAME="Nuclei"/><MEASUREMENT EXPORT_GLOBAL="false" EXPORT_INDIVIDUAL="true" EXPORT_MAX="true" EXPORT_MEAN="true" EXPORT_MIN="true" EXPORT_STD="true" EXPORT_SUM="true" NAME="SPOT_DETECT // RADIUS_(PX)" NICKNAME="SPOT_DETECT // RADIUS_(PX)" OBJECT_NAME="Nuclei"/><MEASUREMENT EXPORT_GLOBAL="false" EXPORT_INDIVIDUAL="true" EXPORT_MAX="true" EXPORT_MEAN="true" EXPORT_MIN="true" EXPORT_STD="true" EXPORT_SUM="true" NAME="SPOT_DETECT // RADIUS_(µm)" NICKNAME="SPOT_DETECT // RADIUS_(µm)" OBJECT_NAME="Nuclei"/><MEASUREMENT EXPORT_GLOBAL="true" EXPORT_INDIVIDUAL="true" EXPORT_MAX="true" EXPORT_MEAN="true" EXPORT_MIN="true" EXPORT_STD="true" EXPORT_SUM="true" NAME="SPOT_DETECT // X_CENTROID_(PX)" NICKNAME="SPOT_DETECT // X_CENTROID_(PX)" OBJECT_NAME="Nuclei"/><MEASUREMENT EXPORT_GLOBAL="false" EXPORT_INDIVIDUAL="true" EXPORT_MAX="true" EXPORT_MEAN="true" EXPORT_MIN="true" EXPORT_STD="true" EXPORT_SUM="true" NAME="SPOT_DETECT // X_CENTROID_(µm)" NICKNAME="SPOT_DETECT // X_CENTROID_(µm)" OBJECT_NAME="Nuclei"/><MEASUREMENT EXPORT_GLOBAL="true" EXPORT_INDIVIDUAL="true" EXPORT_MAX="true" EXPORT_MEAN="true" EXPORT_MIN="true" EXPORT_STD="true" EXPORT_SUM="true" NAME="SPOT_DETECT // Y_CENTROID_(PX)" NICKNAME="SPOT_DETECT // Y_CENTROID_(PX)" OBJECT_NAME="Nuclei"/><MEASUREMENT EXPORT_GLOBAL="false" EXPORT_INDIVIDUAL="true" EXPORT_MAX="true" EXPORT_MEAN="true" EXPORT_MIN="true" EXPORT_STD="true" EXPORT_SUM="true" NAME="SPOT_DETECT // Y_CENTROID_(µm)" NICKNAME="SPOT_DETECT // Y_CENTROID_(µm)" OBJECT_NAME="Nuclei"/><MEASUREMENT EXPORT_GLOBAL="false" EXPORT_INDIVIDUAL="true" EXPORT_MAX="true" EXPORT_MEAN="true" EXPORT_MIN="true" EXPORT_STD="true" EXPORT_SUM="true" NAME="SPOT_DETECT // Z_CENTROID_(SLICE)" NICKNAME="SPOT_DETECT // Z_CENTROID_(SLICE)" OBJECT_NAME="Nuclei"/><MEASUREMENT EXPORT_GLOBAL="false" EXPORT_INDIVIDUAL="true" EXPORT_MAX="true" EXPORT_MEAN="true" EXPORT_MIN="true" EXPORT_STD="true" EXPORT_SUM="true" NAME="SPOT_DETECT // Z_CENTROID_(µm)" NICKNAME="SPOT_DETECT // Z_CENTROID_(µm)" OBJECT_NAME="Nuclei"/></OBJECT_MEASUREMENTS><METADATA/></MODULE><MODULE CLASSNAME="io.github.mianalysis.mia.module.objects.relate.TrackObjects" DISABLEABLE="false" ENABLED="true" ID="1681800188542" NAME="Track objects" NICKNAME="Track objects" NOTES="Also using TrackMate, the &quot;Track objects&quot; module takes any previously-detected objects and links them through time.  Tracked objects can have been detected by any upstream module, not just the &quot;Spot detection&quot; module.&#10;&#10;For each detected track, a &quot;Track&quot; object will be created.  This object contains no coordinate information, but rather acts as a parent object to all the associated &quot;Input objects&quot;.  As such, each &quot;Nuclei&quot; object in that track is a child of the track object.  Here, the track objects are simply called &quot;Tracks&quot;.&#10;&#10;By default, the &quot;Track objects&quot; module will optimse the links based on the distance between object centroids.  It's also possible to allow tracks to go undetected for a certain number of frames (the &quot;Maximum number of missing frames&quot; parameter).&#10;&#10;Beyond minimising the overall distance between object centroids, the &quot;Track objects&quot; module also allows minimising metrics such as difference in object volume and difference in track orientation.  In this example, these additional weights have been disabled.  To learn more, please see the module documentation." SHOW_BASIC_TITLE="true" SHOW_OUTPUT="false" VERSION="1.0.0"><PARAMETERS><PARAMETER NAME="Input objects" NICKNAME="Input objects" VALUE="Nuclei" VISIBLE="false"/><PARAMETER NAME="Output track objects" NICKNAME="Output track objects" VALUE="Tracks" VISIBLE="false"/><PARAMETER NAME="Linking method" NICKNAME="Linking method" VALUE="Centroid" VISIBLE="false"/><PARAMETER NAME="Minimum overlap" NICKNAME="Minimum overlap" VALUE="1.0" VISIBLE="false"/><PARAMETER NAME="Maximum linking distance (px)" NICKNAME="Maximum linking distance (px)" VALUE="10" VISIBLE="false"/><PARAMETER NAME="Maximum number of missing frames" NICKNAME="Maximum number of missing frames" VALUE="0" VISIBLE="false"/><PARAMETER NAME="Frame gap weighting" NICKNAME="Frame gap weighting" VALUE="0.0" VISIBLE="false"/><PARAMETER NAME="Favour established tracks" NICKNAME="Favour established tracks" VALUE="false" VISIBLE="false"/><PARAMETER NAME="Track length weighting" NICKNAME="Track length weighting" VALUE="1.0" VISIBLE="false"/><PARAMETER NAME="Use volume (minimise volume change)" NICKNAME="Use volume (minimise volume change)" VALUE="false" VISIBLE="false"/><PARAMETER NAME="Volume weighting" NICKNAME="Volume weighting" VALUE="1.0" VISIBLE="false"/><PARAMETER NAME="Maximum volume change (px^3)" NICKNAME="Maximum volume change (px^3)" VALUE="1000" VISIBLE="false"/><PARAMETER NAME="Direction weighting mode" NICKNAME="Direction weighting mode" VALUE="None" VISIBLE="false"/><PARAMETER NAME="Orientation range mode" NICKNAME="Orientation range mode" VALUE="-180 to 180 degs" VISIBLE="false"/><PARAMETER NAME="Preferred direction" NICKNAME="Preferred direction" VALUE="0.0" VISIBLE="false"/><PARAMETER NAME="Direction tolerance" NICKNAME="Direction tolerance" VALUE="180" VISIBLE="false"/><PARAMETER NAME="Direction weighting" NICKNAME="Direction weighting" VALUE="1.0" VISIBLE="false"/><PARAMETER NAME="Use measurement (minimise change)" NICKNAME="Use measurement (minimise change)" VALUE="false" VISIBLE="false"/><PARAMETER NAME="Measurement" NICKNAME="Measurement" VALUE="" VISIBLE="false"/><PARAMETER NAME="Measurement weighting" NICKNAME="Measurement weighting" VALUE="1.0" VISIBLE="false"/><PARAMETER NAME="Maximum measurement change" NICKNAME="Maximum measurement change" VALUE="1.0" VISIBLE="false"/></PARAMETERS><IMAGE_MEASUREMENTS/><OBJECT_MEASUREMENTS><MEASUREMENT EXPORT_GLOBAL="false" EXPORT_INDIVIDUAL="true" EXPORT_MAX="true" EXPORT_MEAN="true" EXPORT_MIN="true" EXPORT_STD="true" EXPORT_SUM="true" NAME="TRACKING // NEXT_OBJECT_IN_TRACK_ID" NICKNAME="TRACKING // NEXT_OBJECT_IN_TRACK_ID" OBJECT_NAME="Nuclei"/><MEASUREMENT EXPORT_GLOBAL="false" EXPORT_INDIVIDUAL="true" EXPORT_MAX="true" EXPORT_MEAN="true" EXPORT_MIN="true" EXPORT_STD="true" EXPORT_SUM="true" NAME="TRACKING // PREVIOUS_OBJECT_IN_TRACK_ID" NICKNAME="TRACKING // PREVIOUS_OBJECT_IN_TRACK_ID" OBJECT_NAME="Nuclei"/></OBJECT_MEASUREMENTS><METADATA/></MODULE><MODULE CLASSNAME="io.github.mianalysis.mia.module.objects.measure.miscellaneous.ChildObjectCount" DISABLEABLE="false" ENABLED="true" ID="1694380001145" NAME="Child object count" NICKNAME="Child object count" NOTES="Badly-detected tracks may only contain a few points.  We can filter these out by calculating the number of child &quot;Nuclei&quot; objects associated with each &quot;Track&quot; object.&#10;&#10;The &quot;Child object count&quot; module adds a measurement to each parent object (the &quot;Track&quot; objects in this case) reporting how many children there are.&#10;&#10;Unlike normal measurements, the object count measurements are dynamic; that is, they're calculated live, so will keep track if children are added or remove from parent objects." SHOW_BASIC_TITLE="true" SHOW_OUTPUT="false" VERSION="1.0.0"><PARAMETERS><PARAMETER NAME="Input objects" NICKNAME="Input obj